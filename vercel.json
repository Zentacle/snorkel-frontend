{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://snorkel-backend.herokuapp.com/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://snorkel-backend.herokuapp.com/:path*/"
    },
    {
      "source": "/image/:path*",
      "destination": "https://snorkel.s3.amazonaws.com/:path*"
    }
  ]
}